# This is a template for your .env.local file
# Copy this file to .env.local and replace the placeholders with your actual values
# DO NOT COMMIT .env.local TO VERSION CONTROL

# WalletConnect Project ID - Get one at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLET_CONECT_PROJECT_ID=your_wallet_connect_project_id_here

# IPFS Configuration
NEXT_PUBLIC_INFURA_IPFS_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_INFURA_IPFS_PROJECT_SECRET=your_project_secret_here

# Web3 Configuration
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# MongoDB Configuration
MONGODB_URI=your_mongodb_uri_here
MONGODB_DB_NAME=your_mongodb_db_name_here

# Groq API Configuration - Get your key at https://console.groq.com/keys
NEXT_PUBLIC_GROQ_API_KEY=your_groq_api_key_here

# Google Imagen API Configuration - For comic style story image generation
NEXT_PUBLIC_GOOGLE_IMAGEN_API_KEY=your_google_imagen_api_key_here

# Stability AI Configuration - For AI-generated visuals
NEXT_PUBLIC_STABILITY_AI_API_KEY=your_stability_ai_api_key_here

# Monad Configuration (for NFT minting)
MONAD_RPC_URL=your_monad_rpc_url_here
MINTER_PRIVATE_KEY=your_minter_private_key_here

# Unsplash API Key for image placeholders
NEXT_PUBLIC_UNSPLASH_API_KEY=your_unsplash_api_key_here

# Monad Network ID for blockchain interactions
NEXT_PUBLIC_MONAD_NETWORK_ID=your_monad_network_id_here

# Other potential environment variables (optional, depending on your setup)
# NEXT_PUBLIC_WEB3_PROVIDER_URL=your_web3_provider_url_here
# NEXT_PUBLIC_CONTRACT_ADDRESS=your_contract_address_here

# Note: Do not commit .env.local with actual keys to version control
# Keep sensitive information secure and only use this file as a guide

# Environment variables for local development

# Set this to true during build to mock or disable database connections
NEXT_PUBLIC_BUILD_MODE=false

# Pinata API Credentials for IPFS Storage
NEXT_PUBLIC_PINATA_API_KEY=your_pinata_api_key_here
PINATA_API_SECRET=your_pinata_api_secret_here
PINATA_JWT=your_pinata_jwt_here

# Storacha Key
NEXT_PUBLIC_STORACHA_KEY=your_storacha_key_here 