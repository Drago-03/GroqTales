# This is a template for your .env.local file
# Copy this file to .env.local and replace the placeholders with your actual values
# DO NOT COMMIT .env.local TO VERSION CONTROL

# WalletConnect Project ID - Get one at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLET_CONECT_PROJECT_ID=your_wallet_connect_project_id

# IPFS Configuration
NEXT_PUBLIC_INFURA_IPFS_PROJECT_ID=your_infura_ipfs_project_id
NEXT_PUBLIC_INFURA_IPFS_PROJECT_SECRET=your_infura_ipfs_secret

# Web3 Configuration
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# MongoDB Configuration
MONGODB_URI=your_mongodb_connection_string
MONGODB_DB_NAME=your_db_name

# Groq API Configuration - Get your key at https://console.groq.com/keys
NEXT_PUBLIC_GROQ_API_KEY=your_groq_api_key

# Monad Configuration (for NFT minting)
MONAD_RPC_URL=https://rpc.testnet.monad.xyz/json-rpc
MINTER_PRIVATE_KEY=your_private_key_for_minting 